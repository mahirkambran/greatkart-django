container_commands:
  01_migrate:
    command: "cd /var/app/current && source /var/app/venv/*/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true
  02_collectstatic:
    command: "cd /var/app/current && source /var/app/venv/*/bin/activate && python3 manage.py collectstatic --noinput"
    leader_only: true

